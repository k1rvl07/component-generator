{
  "name": "component-generator",
  "displayName": "Component Generator",
  "description": "A VS Code extension to generate React components using Plop.",
  "version": "1.0.0",
  "publisher": "k1rvl_",
  "engines": {
    "vscode": "^1.96.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.createComponent"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.createComponent",
        "title": "Create React Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.createComponent",
          "group": "navigation",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "echo No compilation needed for JS",
    "watch": "echo No watch needed for JS",
    "pretest": "npm run compile",
    "test": "echo No tests yet"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/vscode": "^1.96.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/your-repo.git"
  },
  "license": "MIT"
}